<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_catalog"
    app:startDestination="@id/CatalogFragment">

    <fragment
        android:id="@+id/CatalogFragment"
        android:name="com.example.exhaustwear.fragments.catalog.CatalogFragment"

        tools:layout="@layout/fragment_catalog" >
        <action
            android:id="@+id/action_CatalogFragment_to_testFragment"
            app:destination="@id/testFragment" />
    </fragment>
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.exhaustwear.fragments.catalog.TestFragment"

        tools:layout="@layout/fragment_test" >
        <action
            android:id="@+id/action_testFragment_to_CatalogFragment"
            app:destination="@id/CatalogFragment" />
    </fragment>
</navigation>